    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBrBtIogaQ2lklgpqhAc3XXmEOqXdI_U4s" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapTestActivity" />
        <activity android:name=".CameraTestActivity" />
        <activity android:name=".CloudTestActivity" />
        <activity android:name=".UITestActivity" />
        <activity android:name=".ObjectStructureActivity" />
        <activity
            android:name=".viewPostActivity"
            android:label="@string/title_activity_view_post"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".viewPostStatActivity"></activity>
    </application>
</manifest>